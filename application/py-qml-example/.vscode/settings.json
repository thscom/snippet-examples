{
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/Scripts/python.exe",
  "qtForPython.designer.path": "${workspaceFolder}/.venv/Scripts/pyside6-designer.exe",
  "qtForPython.linguist.path": "${workspaceFolder}/.venv/Scripts/pyside6-linguist.exe",
  "qtForPython.lrelease.path": "${workspaceFolder}/.venv/Scripts/pyside6-lrelease.exe",
  "qtForPython.lupdate.path": "${workspaceFolder}/.venv/Scripts/pyside6-lupdate.exe",
  "qtForPython.qml.path": "${workspaceFolder}/.venv/Scripts/pyside6-qml.exe",
  "qtForPython.qmlformat.path": "${workspaceFolder}/.venv/Scripts/pyside6-qmlformat.exe",
  "qtForPython.qmlls.enabled": false,
  "qtForPython.qmlls.path": "${workspaceFolder}/.venv/Scripts/pyside6-qmlls.exe",
  "qtForPython.qmlls.options": ["-v"],
  "qtForPython.rcc.path": "${workspaceFolder}/.venv/Scripts/pyside6-rcc.exe",
  "qtForPython.uic.path": "${workspaceFolder}/.venv/Scripts/pyside6-uic.exe",
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultColorDecorators": "auto",
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.formatOnSave": true,
    "editor.wordBasedSuggestions": "off"
  }
}
